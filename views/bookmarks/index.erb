<form action='/bookmarks' method='post'>
<input type='text' name='title' placeholder='title' />
<input type='text' name='url' placeholder='url' />
<input type='submit' value='Add bookmark' />
</form>

<ul>
  <% @bookmarks.each do |bookmark| %>
  <li>
      <a href="<%= bookmark.url %>">
        <%= bookmark.title %>
      </a>
    </li>
  <% end %>
</ul>
